<%=form_with url: sign_in_path do |f|%>
<div class="mb-3">
        <%=f.label :email%>
        <%=f.text_field :email,class:"form-control"%>
    </div>
    <div class="mb-3">
        <%=f.label :password%>
        <%=f.password_field :password,class:"form-control"%>
    </div>
   <div>
   <%=f.submit "sign In" %>
   </div>
<% end %>
